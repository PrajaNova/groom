{
    "name": "groom-ms",
    "version": "1.0.0",
    "description": "Content Management Microservice for Groom",
    "main": "dist/app.js",
    "type": "commonjs",
    "scripts": {
        "start": "node dist/app.js",
        "build": "tsc",
        "watch:tsc": "tsc -w",
        "dev": "tsx watch --clear-screen=false src/server.ts",
        "typecheck": "tsc --noEmit",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "test": "vitest"
    },
    "keywords": [
        "groom",
        "microservice",
        "fastify",
        "cms"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@asteasolutions/zod-to-openapi": "catalog:",
        "@fastify/autoload": "catalog:",
        "@fastify/cors": "catalog:",
        "@fastify/helmet": "catalog:",
        "@fastify/sensible": "catalog:",
        "@prisma/client": "catalog:",
        "dotenv": "catalog:",
        "fastify": "catalog:",
        "fastify-plugin": "catalog:",
        "jsonwebtoken": "^9.0.3",
        "uuid-random": "^1.3.2",
        "zod": "catalog:",
        "zod-to-json-schema": "catalog:"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "catalog:",
        "prisma": "catalog:",
        "tsconfig-paths": "catalog:",
        "tsx": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:"
    }
}
